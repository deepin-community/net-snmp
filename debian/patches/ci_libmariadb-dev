-		       libmariadbclient-dev libmariadb-client-lgpl-dev; do
+	      for p in libmariadb-dev-compat; do
 	        dpkg_version "$p" && break;
               done;
               pacman_version mingw-w64-x86_64-libmariadbclient)"
